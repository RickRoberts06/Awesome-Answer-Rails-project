<h1> This is the contact page ;) </h1>

<%= form_tag "/contact" do %>
    <div>
      <%= label_tag :name %>
      <%= text_field_tag :name, "", placeholder: "e.g. Eric" %>
    </div>
    <div>
      <%= label_tag :email %>
      <%= email_field_tag :email %>
    </div>
    <%= submit_tag %>
<% end %>
